<p id="notice"><%= notice %></p>

<h1>Store Names</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @store_names.each do |store_name| %>
      <tr>
        <td><%= link_to 'Show', store_name %></td>
        <td><%= link_to 'Edit', edit_store_name_path(store_name) %></td>
        <td><%= link_to 'Destroy', store_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store Name', new_store_name_path %>
